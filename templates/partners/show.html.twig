{% extends 'base.html.twig' %}

{% block title %}Partners{% endblock %}

{% block body %}
    <h1>Partners</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partner.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ partner.name }}</td>
            </tr>
            <tr>
                <th>LogoURL</th>
                <td>{{ partner.logoURL }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ partner.address }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ partner.phone }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ partner.email }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ partner.website }}</td>
            </tr>
            <tr>
                <th>Lng</th>
                <td>{{ partner.lng }}</td>
            </tr>
            <tr>
                <th>Lat</th>
                <td>{{ partner.lat }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_partners_index') }}">back to list</a>

    <a href="{{ path('app_partners_edit', {'id': partner.id}) }}">edit</a>

    {{ include('partners/_delete_form.html.twig') }}
{% endblock %}
